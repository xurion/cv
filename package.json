  "homepage": "https://xurion.github.io/cv",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "lint": "eslint .",
    "loki": "loki --requireReference",
