    "loki": "^0.35.1",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "prettier": "3.3.3",
    "storybook": "^8.3.5",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
